    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "5bfa43ca-5ab8-4bc4-8b56-6895df2dbdc4"
